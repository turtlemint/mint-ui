{
    "name": "tm-components",
    "version": "0.1.2",
    "description": "react, react-ui, react-base-components, react-library, react-ui-library",
    "author": "Kushal V. Mahajan",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://gitlab.com/turtlemint/tm-components.git",
    "dependencies": {
        "@babel/core": "7.4.3",
        "@svgr/webpack": "4.1.0",
        "@types/jest": "24.0.13",
        "@types/node": "12.0.4",
        "@types/react": "16.8.19",
        "@types/react-dom": "16.8.4",
        "@typescript-eslint/eslint-plugin": "1.6.0",
        "@typescript-eslint/parser": "1.6.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "^24.8.0",
        "babel-loader": "8.0.5",
        "babel-plugin-named-asset-import": "^0.3.2",
        "babel-preset-react-app": "^9.0.0",
        "camelcase": "^5.2.0",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "css-loader": "2.1.1",
        "dotenv": "6.2.0",
        "dotenv-expand": "4.2.0",
        "eslint": "^5.16.0",
        "eslint-config-react-app": "^4.0.1",
        "eslint-loader": "2.1.2",
        "eslint-plugin-flowtype": "2.50.1",
        "eslint-plugin-import": "2.16.0",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-react": "7.12.4",
        "eslint-plugin-react-hooks": "^1.5.0",
        "file-loader": "3.0.1",
        "fs-extra": "7.0.1",
        "html-webpack-plugin": "4.0.0-beta.5",
        "identity-obj-proxy": "3.0.0",
        "immutable": "4.0.0-rc.12",
        "is-wsl": "^1.1.0",
        "jest": "24.7.1",
        "jest-dom": "^3.4.0",
        "jest-environment-jsdom-fourteen": "0.1.0",
        "jest-resolve": "24.7.1",
        "jest-watch-typeahead": "0.3.0",
        "lodash": "4.17.11",
        "mini-css-extract-plugin": "0.5.0",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "pnp-webpack-plugin": "1.2.1",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "7.0.1",
        "postcss-preset-env": "6.6.0",
        "postcss-safe-parser": "4.0.1",
        "react": "16.8.6",
        "react-app-polyfill": "^1.0.1",
        "react-dev-utils": "^9.0.1",
        "react-dom": "16.8.6",
        "react-native-web": "^0.11.5",
        "resolve": "1.10.0",
        "sass-loader": "7.1.0",
        "semver": "6.0.0",
        "source-map-explorer": "^2.0.0",
        "style-loader": "0.23.1",
        "styled-components": "4.2.1",
        "terser-webpack-plugin": "1.2.3",
        "ts-pnp": "1.1.2",
        "typescript": "^3.5.1",
        "url-loader": "1.1.2",
        "webpack": "4.29.6",
        "webpack-dev-server": "3.2.1",
        "webpack-manifest-plugin": "2.0.4",
        "workbox-webpack-plugin": "4.2.0"
    },
    "publishConfig": {
        "registry": "http://nexus.turtlemint.com/repository/npm-private/"
    },
    "scripts": {
        "start": "env-cmd -e development node scripts/start.js ",
        "build": "env-cmd -e production node scripts/build.js",
        "build:staging": "env-cmd -e staging node scripts/build.js",
        "build:lib": "rm -rf dist && tsc",
        "test": "node scripts/test.js",
        "lint": "tslint --project tsconfig.json",
        "lint-fix": "tslint --project tsconfig.json --fix",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "storybook": "start-storybook -p 9001 -c .storybook"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.1.1",
        "@storybook/addon-info": "^5.1.1",
        "@storybook/addon-knobs": "^5.1.1",
        "@storybook/react": "^5.0.11",
        "@testing-library/react": "^8.0.1",
        "@types/lodash": "^4.14.133",
        "@types/storybook__addon-info": "^4.1.1",
        "@types/storybook__addon-knobs": "^5.0.0",
        "@types/storybook__react": "^4.0.1",
        "@types/styled-components": "^4.1.16",
        "env-cmd": "^9.0.2",
        "husky": "^2.3.0",
        "react-docgen-typescript-loader": "^3.1.0",
        "react-hooks-testing-library": "^0.5.1",
        "react-test-renderer": "^16.8.6",
        "storybook-addon-jsx": "^7.1.2",
        "ts-loader": "^6.0.2",
        "tslint": "^5.17.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "sideEffects": [
        "./src/setupTests"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "yarn run lint && yarn run lint-fix"
        }
    },
    "files": [
        "README.MD",
        "dist/**"
    ]
}
